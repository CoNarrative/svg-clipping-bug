        </marker>
        <marker id="marker-arrow-line-widget" viewBox="0 0 10 10" refX="9" refY="5" markerUnits="strokeWidth"
                orient="auto" markerWidth="12" markerHeight="9">
            <polyline points="0,0 10,5 0,10 1,5"></polyline>
        </marker>
        <marker id="marker-arrow-plain" viewBox="0 0 10 10" refX="1" refY="5" markerUnits="strokeWidth" orient="auto"
