            <polyline fill="#003800" points="0,0 10,5 0,10 1,5"></polyline>
        </marker>
        <marker id="marker-arrow-selected" viewBox="0 0 10 10" refX="1" refY="5" markerUnits="strokeWidth" orient="auto"
